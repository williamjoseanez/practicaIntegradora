{{#each products}}

  <div class="card">
    <img src="{{this.thumbnail}}" class="card-img-top" alt="{{this.title}}" />
    <p>Title: {{this.title}}</p>
    <p>Description: {{this.description}}</p>
    <p>Price: $ {{this.price}}</p>
    <p>Category: {{this.category}}</p>

    <button>Comprar</button>

  </div>

{{/each}}